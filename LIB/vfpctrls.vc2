			this.cmdbutton&iString..caption=aButtonNames[i]
			this.cmdbutton&iString..visible=.T.
		NEXT
		
		lcColor = rgbscheme(1, 4)
		lcColor = 'rgb(' + substr(lcColor, at(',', lcColor, 3) + 1)
		this.FillColor = &lcColor
	ENDPROC

