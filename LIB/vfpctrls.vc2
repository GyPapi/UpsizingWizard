	PROCEDURE Init
		PARAMETERS lcMessageText, aButtonNames
		LOCAL i, lcColor
		
		this.caption=TITLE_TEXT_LOC
		this.wizlabel1.caption=lcMessageText
		
		*put captions on buttons
		FOR i=1 TO ALEN(aButtonNames,1)
